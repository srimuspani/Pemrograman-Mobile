        android:supportsRtl="true"
        android:theme="@style/Theme.LaundryApp">
        <activity
            android:name=".view.main.DashboardActivity"
            android:exported="true" />
        <activity
            android:name=".view.main.LoginActivity"
            android:exported="true">
