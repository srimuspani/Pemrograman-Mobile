        <activity
            android:name=".view.main.SignUpActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.history.HistoryActivity"
            android:exported="true" />
        <activity
            android:name=".view.CuciBasahActivity"
            android:exported="true" />
        <activity
            android:name="view.DryCleanActivity"
            android:exported="true" />
        <activity
            android:name=".view.IroningActivity"
            android:exported="true" />
        <activity
            android:name=".view.PremiumWashActivity"
            android:exported="true" />
    </application>

</manifest>